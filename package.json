    "http-ask": "^0.0.0-rc1",
    "http-proxy": "^1.12.0",
    "koa": "^2.2.0",
    "koa-body": "^1.2.1",
    "koa-bodyparser": "^2.0.1",
    "koa-conditional-get": "^1.0.3",
    "koa-connect-history-api-fallback": "0.3.0",
    "koa-error": "^3.0.0",
    "koa-etag": "^2.0.0",
    "koa-favicon": "^2.0.0",
    "koa-gzip": "^0.1.0",
