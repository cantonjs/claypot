{
  "name": "claypot",
  "description": "Just a static HTTP server.",
  "version": "0.0.0-beta.15",
  "main": "lib/index",
  "bin": {
    "claypot": "bin/claypot"
  },
  "scripts": {
    "start": "yarn cli -- start -f",
    "ls": "yarn cli -- ls",
    "log": "yarn cli -- log",
    "stop": "yarn cli -- stop",
