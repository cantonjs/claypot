    "build": "babel src -d lib",
    "prepublish": "yarn build",
    "cli": "cross-env CLAYPOT_DEV=true babel-node src/cli",
    "test": "yarn build && yarn cli"
  },
  "author": "Cap32",
  "licenses": "MIT",
  "engines": {
    "node": ">= 7.6.0"
  },
